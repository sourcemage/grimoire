           SPELL=qalculate-kde
         VERSION=0.8.1.1
          SOURCE=$SPELL-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=$SOURCEFORGE_URL/qalculate/$SOURCE
          MD5[0]=a58301d29e9a124327865aefcc4f1031
        WEB_SITE=http://qalculate.sourceforge.net/
