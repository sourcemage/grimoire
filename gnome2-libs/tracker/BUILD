OPTS="--enable-generic-media-extractor=${TRACKER_EXTRACTOR} \
      ${TRACKER_SEARCH_TOOL}                        \
      ${TRACKER_PREFERENCES}                        \
      ${TRACKER_STATUS}                             \
      ${TRACKER_EXPLORER}                           \
      ${TRACKER_SEARCH_BAR}                         \
