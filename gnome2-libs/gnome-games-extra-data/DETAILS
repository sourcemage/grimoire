           SPELL=gnome-games-extra-data
         VERSION=2.12.0
          BRANCH=`echo $VERSION|cut -d . -f 1,2`
	  SOURCE=$SPELL-$VERSION.tar.bz2
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=$GNOME_URL/sources/$SPELL/$BRANCH/$SOURCE
      SOURCE_GPG="gurus.gpg:${SOURCE}.sig"
      LICENSE[0]=GPL
      LICENSE[1]=FDL
        WEB_SITE=http://www.gnome.org
         ENTERED=20020508
         UPDATED=20050318
        KEYWORDS="gnome2 libs"
           SHORT="extra data files for the gnome-games packag"
cat << EOF
This package contains extra data files for the gnome-games package.
At the moment this is almost entirely extra themes and artwork for
the games.
EOF
