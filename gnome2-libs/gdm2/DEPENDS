depends  libgnomeui       &&
depends  libglade2        &&
depends  librsvg2         &&
depends  rarian           &&
depends  gnome-doc-utils  &&

if  [  "$(get_spell_provider  gtk+2  X11-LIBS)"  ==  "xorg-server"  ];  then
  optional_depends  sessreg  ''  ''  'for wtmp/utmp updating'
fi  &&

if [ $DBUSLAUNCH == "y" ]; then
  depends dbus
fi  &&

if echo $GDM_SECURE | grep -q "yes$" ; then
  depends zenity
fi
