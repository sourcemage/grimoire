   is_version_less "$(installed_version at-spi2-core)" "2.45.1"; then
  depends atk
fi &&
depends dbus &&
depends dbus-glib &&
depends glib2 &&
depends intltool &&
depends libice &&
depends libsm &&
depends libx11 &&
depends libxau &&
depends libxcb &&
depends libxdmcp &&
depends libxml2 &&
depends util-linux
