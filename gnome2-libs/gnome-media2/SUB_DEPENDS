case $THIS_SUB_DEPENDS in
    PROFILES) if [[ $GMEDIA_GMP != y ]]; then
                message "GNOME media profiles requested, forcing dependency" &&
                GMEDIA_GMP=y
              fi
              ;;
           *) message "${PROBLEM_COLOR}bogus sub_depends: $SPELL $THIS_SUB_DEPENDS${DEFAULT_COLOR}" ;
              return 1;;
esac
