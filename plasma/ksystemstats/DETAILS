source $GRIMOIRE/KDE_VERSIONS
           SPELL=ksystemstats
         VERSION=$PLASMA_VERSION
          SOURCE=$SPELL-$VERSION.tar.xz
      SOURCE_URL=https://download.kde.org/$PLASMA_BRANCH/plasma/${VERSION}/$SOURCE
         SOURCE2=$SOURCE.sig
  SOURCE2_URL[0]=${SOURCE_URL[0]}.sig
      SOURCE_GPG=kde.gpg:$SOURCE.sig:UPSTREAM_KEY
  SOURCE2_IGNORE=signature
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
        WEB_SITE=https://quickgit.kde.org/?p=ksystemstats.git
       LICENSE[0]=GPL
         ENTERED=20210420
        KEYWORDS="KDE5"
           SHORT="collects statistics about the running system"
cat << EOF
KSystemStats is a daemon that collects statistics about the running system.
EOF
