source $GRIMOIRE/KDE_VERSIONS
           SPELL=kalendar
         VERSION=$KDE_VERSION
          SOURCE=$SPELL-$VERSION.tar.xz
      SOURCE_URL=https://download.kde.org/$KDE_BRANCH/release-service/$VERSION/src/$SOURCE
         SOURCE2=$SOURCE.sig
  SOURCE2_URL[0]=${SOURCE_URL[0]}.sig
      SOURCE_GPG=kde.gpg:$SOURCE.sig:UPSTREAM_KEY
  SOURCE2_IGNORE=signature
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
        WEB_SITE=https://github.com/KDE/kalendar
         ENTERED=20220307
#  SECURITY_PATCH=1
      LICENSE[0]=LGPL
        KEYWORDS="kde5"
           SHORT=""
cat << EOF
A calendar application using Akonadi to sync with external services (Nextcloud, GMail, ...)
EOF
