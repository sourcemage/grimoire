depends  libtool    &&
depends  SSL    &&
depends  libgcrypt  &&
depends  gnutls     &&
depends -sub CXX gcc &&
if list_find "$GWEN_GUI" qt5;then 
 depends qtbase
fi &&
if list_find "$GWEN_GUI" gtk2;then 
 depends gtk+2
fi &&
if list_find "$GWEN_GUI" gtk3;then 
 depends gtk+3
fi &&
optional_depends ncurses  "" "" "to provide a text gui"
