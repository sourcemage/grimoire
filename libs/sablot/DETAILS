           SPELL=sablot
         VERSION=1.0.3
          SOURCE=Sablot-$VERSION.tar.gz
SOURCE_DIRECTORY="$BUILD_DIRECTORY/Sablot-$VERSION"
   SOURCE_URL[0]=$SOURCEFORGE_URL/sablotron/$SOURCE
     SOURCE_HASH=sha512:836c1a9d09317194333619461b0e5ea312277cadb6df5ae5ed327bdfee4090970bd3056ec0b3bd6aa8a3e3289af1bc5c1461c9fe15b53a944f1042190c485031
