           SPELL=sablot
         VERSION=1.0.2
          SOURCE=Sablot-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/Sablot-$VERSION
   SOURCE_URL[0]=http://download-1.gingerall.cz/download/$SPELL/$SOURCE
          MD5[0]='6ec39729862e44149181be51eb298f1e'
