           SPELL=libcdio-paranoia
         VERSION=10.2+0.93+1
      PATCHLEVEL=0
          SOURCE=$SPELL-$VERSION.tar.bz2
         SOURCE2=$SOURCE.sig
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=${GNU_URL}/libcdio/$SOURCE
  SOURCE2_URL[0]=${GNU_URL}/libcdio/$SOURCE2
