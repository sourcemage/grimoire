default_pre_build &&
cd "$SOURCE_DIRECTORY" &&
if [[ "${LIBOAUTH_BRANCH}" == "stable" ]]; then
  sedit 's!maybe_os in$!& linux-musl*|\\!' config.sub
fi &&
if [[ "${LIBOAUTH_BRANCH}" == "scm" ]]; then
  touch ltconfig &&
  libtoolize --force --copy --automake &&
  aclocal -I m4 &&
  autoheader &&
  automake --foreign --copy --add-missing &&
  touch depcomp &&
  autoconf
fi
