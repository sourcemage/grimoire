source $GRIMOIRE/CMAKE_DEPENDS &&
depends -sub CXX gcc      &&
depends libxml2  &&
depends eigen3   &&
depends boost   &&
depends rapidjson &&
optional_depends cairo '' '' 'support PNG output' &&
optional_depends WXWIDGET '' '-DBUILD_GUI=0' 'build a GUI'  &&
optional_depends zlib '' '' 'compression support'
