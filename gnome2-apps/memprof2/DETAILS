          SOURCE=memprof-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/memprof-$VERSION
   SOURCE_URL[0]=${GNOME_URL}/sources/memprof/${BRANCH}/${SOURCE}
      SOURCE_GPG="gurus.gpg:${SOURCE}.sig"
      LICENSE[0]=GPL
        WEB_SITE=http://www.gnome.org/projects/memprof/
