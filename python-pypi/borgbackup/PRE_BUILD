default_pre_build
cd $SOURCE_DIRECTORY &&
# allow latest msgpack
sed -i "/msgpack/s/1, 0, 5/1, 0, 7/"  pyproject.toml src/borg/helpers/msgpack.py &&
sed -i "/msgpack/s/1.0.5/1.0.7/"  setup.py &&
# force msgpack version
sed -i '/msgpack/s/1.1.0/1.1.2/' pyproject.toml PKG-INFO src/borgbackup.egg-info/* &&
sed -i '/msgpack.version/s/1, 1, 0/1, 1, 2/' src/borg/helpers/msgpack.py &&
sed -i 's/_scm//' setup.py
