source $GRIMOIRE/CMAKE_DEPENDS &&
depends -sub CXX gcc  &&
optional_depends expat '' '' 'XMP tag support' &&
optional_depends brotli  -DEXIV2_ENABLE_BROTLI={1,0}  'Brotli support' &&
optional_depends GETTEXT -DEXIV2_ENABLE_NLS={1,0}    'NLS support'    &&
optional_depends inih    -DEXIV2_ENABLE_INIH={1,0}   'INIReader support'
