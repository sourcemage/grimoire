OPTS="$XTERM_OPTS $OPTS"  &&
# group used for xterm
#create_group utmp                                             &&

OPTS="$OPTS --x-includes=/usr/include"                        &&
OPTS="$OPTS --x-libraries=/usr/lib"                           &&
OPTS="$OPTS --with-app-defaults=/usr/share/X11/app-defaults"  &&
OPTS="$OPTS --with-utmp-setgid=utmp"                          &&

default_build

