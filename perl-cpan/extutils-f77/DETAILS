           SPELL=extutils-f77
         VERSION=1.14
          SOURCE=ExtUtils-F77-$VERSION.tar.gz
       BUILD_API=2
SOURCE_DIRECTORY=$BUILD_DIRECTORY/ExtUtils-F77-$VERSION
   SOURCE_URL[0]=http://www.cpan.org/authors/id/K/KG/KGB/$SOURCE
          MD5[0]="c3ac0af9163ccd7c69f7b6447c224b37"
