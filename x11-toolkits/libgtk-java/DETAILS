         VERSION=cvs
          SOURCE=$SPELL-$VERSION.tar.gz
   SOURCE_URL[0]=cvs://:pserver:anonymous@anoncvs.gnome.org:/cvs/gnome:java-gnome/libgtk-java
          MD5[0]='IGNORE'
  FORCE_DOWNLOAD=1
else
