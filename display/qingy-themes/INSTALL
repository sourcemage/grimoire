  I_DIR=/etc/qingy/themes                   &&
  [ ! -d "${I_DIR}" ] && mkdir -p "${I_DIR}"
  cp  -a  *  ${I_DIR}                       &&
  cp  -a  ${SCRIPT_DIRECTORY}/sourcemage    \
          ${I_DIR}                          &&
  cp  -a  ${SCRIPT_DIRECTORY}/sorcerous     \
          ${I_DIR}                          &&
  chown -R root.root ${I_DIR}
