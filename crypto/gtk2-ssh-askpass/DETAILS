SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=http://www.cgabriel.org/sw/gtk2-ssh-askpass/$SOURCE
          MD5[0]=22db8d8f575801b22b188d76f7fb4b3e
