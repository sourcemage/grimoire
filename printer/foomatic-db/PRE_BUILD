CURRENT_RELEASE=`tar tzf ${SOURCE_CACHE}/foomatic-db-current.tar.gz | head -1 | cut -d / -f 1`  &&
mk_source_dir  ${BUILD_DIRECTORY}/${CURRENT_RELEASE}                                            &&
unpack         ${SOURCE}  ${MD5[0]}                                                             &&
cd             ${BUILD_DIRECTORY}                                                               &&
ln  -s         ${CURRENT_RELEASE}  foomatic-db-current
