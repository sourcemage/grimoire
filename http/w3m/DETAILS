         VERSION=0.5.5
          SOURCE=$SPELL-${VERSION}.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/${SPELL}-v${VERSION}
   SOURCE_URL[0]=https://git.sr.ht/~rkta/w3m/archive/v$VERSION.tar.gz
     SOURCE_HASH=sha512:ba373483355b46b2a6025e3f3241cd20ac814bd2e47199aa886e55eece7972389d3508cd0d326e89e9630c8b884ba0e3b3462f869eadcf7278115be4247d9fd6
