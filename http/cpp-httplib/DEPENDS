source $GRIMOIRE/MESON_DEPENDS &&
optional_depends SSL     openssl=enabled openssl=disabled 'Enable OpenSSL support' &&
optional_depends zlib    zlib=enabled    zlib=disabled    'Enable zlib support'    &&
optional_depends brotli  brotli=enabled  brotli=disabled  'Enable Brotli support'  &&
optional_depends python3 compile=true    compile=false    'build library'
