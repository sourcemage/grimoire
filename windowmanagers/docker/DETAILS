SOURCE_DIRECTORY=${BUILD_DIRECTORY}/${SPELL}-${VERSION}
   SOURCE_URL[0]=http://icculus.org/openbox/2/docker/${SOURCE}
          MD5[0]=3b0cbcbce5254732e7ae51fd6d0ef493
