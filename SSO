# accounts-sso is tricky to download, standardise it
              SX="${SPELL}_${VERSION}"
          SOURCE="${SX}.tar.bz2"
       SOURCE_URL="https://launchpad.net/ubuntu/+archive/primary/+sourcefiles/${SPELL}/${VERSION}-1/${SX}.orig.tar.bz2"
SOURCE_DIRECTORY="$BUILD_DIRECTORY/${SPELL}-VERSION_${VERSION}"
        WEB_SITE="https://gitlab.com/accounts-sso/$SPELL"
      LICENSE[0]=LGPL
