           SPELL=uucp
         VERSION=1.07
          SOURCE=$SPELL-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=$GNU_URL/uucp/$SOURCE
        WEB_SITE=http://www.airs.com/ian/uucp.html
         ENTERED=20020423
