default_pre_build &&
cd "${SOURCE_DIRECTORY}/subprojects" &&
rm -r hyprland-protocols &&
unpack_file 2 &&
mv "hyprland-protocols-${COMMIT_ID_PROTO}" "hyprland-protocols" &&

cd "${SOURCE_DIRECTORY}" &&

if ! is_depends_enabled "${SPELL}" "qt6base"; then
  sed '/hyprland\-share\-picker/d' -i meson.build
fi
