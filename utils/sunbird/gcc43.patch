-
-    nsresult rv;
-
-    // Initialize XPCOM
-    nsIServiceManager *servMgr = NULL;
-    rv = NS_InitXPCOM2(&servMgr, NULL, NULL);
-    if (NS_FAILED(rv))
-    {
-        // Cannot initialize XPCOM
-        printf("Cannot initialize XPCOM. Exit. [rv=0x%08X]\n", (int)rv);
