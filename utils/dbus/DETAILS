          SOURCE=$SPELL-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=https://dbus.freedesktop.org/releases/dbus/$SOURCE
         SOURCE2=$SOURCE.asc
