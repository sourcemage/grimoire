   SOURCE_URL[0]=https://github.com/gentoo/pax-utils/archive/refs/tags/v$VERSION.tar.gz
     SOURCE_HASH=sha512:4a3c4e8b516591e64befae3eff7424f49ad55721032d1a540d31c14b5d790d4379ca46082ccaf851dd8ac22f9c1904e6a75134f292f009702a1a6cd1cc448e1f
