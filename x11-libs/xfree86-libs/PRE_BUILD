mk_source_dir  $SOURCE_DIRECTORY       &&
unpack         $SOURCE  ${MD5[0]}      &&
unpack         $SOURCE2  ${MD5[1]}     &&
unpack         $SOURCE3  ${MD5[2]}     &&

patch  -d xc -p0  <  $SCRIPT_DIRECTORY/xfree86-xft-releasefile-crash.patch  &&

#recommended by xfree86.org
mk_source_dir  $SOURCE_DIRECTORY.bld 
