-
-	if (parse_table(h) < 0)
-		goto error;
-
-	CHECK(h);
-	return h;
-error:
-	TC_FREE(&h);
-	/* A different process changed the ruleset size, retry */
-	if (errno == EAGAIN)
-		goto retry;
-	return NULL;
-}
