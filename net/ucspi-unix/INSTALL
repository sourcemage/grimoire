mkdir  -p  "${INSTALL_ROOT}/usr/bin"                &&
mkdir  -p  "${INSTALL_ROOT}/usr/share/man"          &&
echo  "${INSTALL_ROOT}/usr/bin"        >  conf-bin  &&
echo  "${INSTALL_ROOT}/usr/share/man"  >  conf-man  &&
make_normal                                         &&
make  installer  instcheck                          &&
./installer                                         &&
./instcheck
