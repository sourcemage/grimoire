if spell_ok maradns && [[ $(installed_version maradns) < 2 ]]
then
  message "This is an ${WARNING_COLOR}INCOMPATIBLE${DEFAULT_COLOR} upgrade to maradns."  &&
  message "Please check: ${WARNING_COLOR}http://www.maradns.org/tutorial/update.html#2.0${DEFAULT_COLOR}"  &&
  message "for more information."  &&

  if ! query "Do you wish to proceed with this upgrade?" n ; then
    return 1
  fi
fi
