           SPELL=linux-initramfs
         VERSION=0.6
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL
        WEB_SITE="http://www.sourcemage.org/"
         ENTERED=20030803
     GATHER_DOCS=off
      PATCHLEVEL=3
        KEYWORDS="kernels"
           SHORT="This spell provides initramfs scripts that can be loaded into the kernel"
cat << EOF
This spell installs some default initramfs scripts that the linux spell can
use to generate initramfs'
EOF
