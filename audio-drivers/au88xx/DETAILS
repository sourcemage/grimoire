          SOURCE=$SPELL-$VERSION.tar.bz2
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
SOURCE_URL[0]=$SOURCEFORGE_URL/aureal/$SOURCE
        WEB_SITE=http://sourceforge.net/projects/aureal
         ENTERED=20011030
