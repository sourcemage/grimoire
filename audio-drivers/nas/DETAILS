           SPELL=nas
if [[ "$NAS_DEV" = "y" ]]; then
         VERSION=1.9a
else
         VERSION=1.9.1
fi
          SOURCE=$SPELL-$VERSION.src.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=$SOURCEFORGE_URL/$SPELL/$SOURCE
      SOURCE_GPG="gurus.gpg:${SOURCE}.sig"
        WEB_SITE=http://radscan.com/nas.html
         ENTERED=20030315
