                 ' -DBUILD_PYTHON_BINDINGS=1' \
                 ' -DBUILD_PYTHON_BINDINGS=0' \
                 'build python bindings' &&
if is_depends_enabled $SPELL pyside6;then
  depends shiboken6 &&
  depends python3
fi
