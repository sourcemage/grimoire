msgid ""
"  --insert file1 [file2]...\n"
"                         open composition window with specified files\n"
"                         inserted"
msgstr ""
"  --insert plik1 [plik2]...\n"
"                         otwórz okno kompozycji z załączonym wybranym\n"
"                         plikiem"

