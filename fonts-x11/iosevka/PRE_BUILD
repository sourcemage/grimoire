mk_source_dir "${SOURCE_DIRECTORY}" &&
cd "${SOURCE_DIRECTORY}" &&

# get everything but license
local srcs="$(get_source_nums | tr ' ' '\n' | head -n -1)" &&

for src in '' ${srcs}; do
  unpack_file ${src} || return 1

  rm -f webfont.css
done &&

verify_file 9 &&

cp "${SOURCE_CACHE}/${SOURCE9}" LICENSE.md
