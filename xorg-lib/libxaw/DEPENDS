depends  gawk                           &&
depends  pkgconfig                      &&
depends  libtool                        &&
depends  util-macros                    &&

if  list_find  "$LIBXAW_OPTS" "--enable-xaw6" ||
    list_find  "$LIBXAW_OPTS" "--enable-xaw7"; then
  depends  xproto                         &&
  depends  libx11                         &&
  depends  libxext                        &&
  depends  libxt                          &&
  depends  libxmu                         &&
  depends  xextproto
fi  &&

if  list_find  "$LIBXAW_OPTS" "--enable-xaw7"; then
  depends  libxpm
fi
