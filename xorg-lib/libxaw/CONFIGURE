config_query_option LIBXAW_OPTS        \
          'Disable building of libXaw.so.6?'      \
          n                                       \
          '--disable-xaw6'                        \
          '--enable-xaw6'                         &&

config_query_option LIBXAW_OPTS        \
          'Disable building of libXaw.so.7?'      \
          n                                       \
          '--disable-xaw7'                        \
          '--enable-xaw7'                     
