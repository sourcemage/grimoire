           SPELL=libfontenc
         VERSION=1.1.8
          SOURCE=${SPELL}-${VERSION}.tar.xz
   SOURCE_URL[0]=https://xorg.freedesktop.org/releases/individual/lib/${SOURCE}
         SOURCE2=$SOURCE.sig
  SOURCE2_URL[0]=${SOURCE_URL[0]}.sig
      SOURCE_GPG=xorg.gpg:$SOURCE.sig:UPSTREAM_KEY
  SOURCE2_IGNORE=signature
SOURCE_DIRECTORY=${BUILD_DIRECTORY}/${SPELL}-${VERSION}
        WEB_SITE=https://xorg.freedesktop.org
         ENTERED=20051125
      LICENSE[0]=XCL
           SHORT="the X.Org font encoding library"
cat << EOF
libfontenc is a library which helps font libraries portably determine
and deal with different encodings of fonts.
EOF
