           SPELL=libxt
             PKG=libXt
         VERSION=1.3.0
  SECURITY_PATCH=1
         SOURCE=${PKG}-${VERSION}.tar.xz
   SOURCE_URL[0]=https://xorg.freedesktop.org/releases/individual/lib/${SOURCE}
         SOURCE2=$SOURCE.sig
  SOURCE2_URL[0]=${SOURCE_URL[0]}.sig
      SOURCE_GPG=xorg.gpg:$SOURCE.sig:UPSTREAM_KEY
  SOURCE2_IGNORE=signature
SOURCE_DIRECTORY=${BUILD_DIRECTORY}/${PKG}-${VERSION}
        WEB_SITE=https://xorg.freedesktop.org
         ENTERED=20051125
      LICENSE[0]=XCL
           SHORT="X Toolkit Intrinsics library"
cat << EOF
libXt is the X Toolkit Intrinsics library used to build older generation
toolkits such as Motif & Xaw
EOF
