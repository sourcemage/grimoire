           SPELL=xdiskusage
         VERSION=1.46
          SOURCE=$SPELL-$VERSION.tgz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=http://xdiskusage.sourceforge.net/$SOURCE
     SOURCE_HASH=sha512:f45f7a6fbd0b3531627c7680aee5b5b44520c5a8a5768062f83fd5b96f1f3b8bda6cc8047f03efe6e25cbb8d81251222f1c5d2fcde9b3049a95d1c87d183f651
