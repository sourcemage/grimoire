           SPELL=transset
if  [  "$TRANSSET_AUTOUPDATE"  ==  "y"  ];  then
         VERSION=$(date +%Y%m%d)
else
         VERSION=cvs
fi
          SOURCE=$SPELL-cvs.tar.bz2
