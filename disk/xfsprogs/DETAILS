           SPELL=xfsprogs
         VERSION=4.5.0
          SOURCE=$SPELL-$VERSION.tar.gz
         SOURCE2=$SOURCE.sig
SOURCE_DIRECTORY="$BUILD_DIRECTORY/$SPELL-$VERSION"
   SOURCE_URL[0]=ftp://oss.sgi.com/projects/xfs/cmd_tars/$SOURCE
