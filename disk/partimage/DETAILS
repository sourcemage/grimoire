   SOURCE_URL[0]=$SOURCEFORGE_URL/$SPELL/$SOURCE
     SOURCE_HASH=sha512:57f1138f2a0f14721f52a65d583ef2e84e51b7eb7c7dd6c6f4dd5535ff473ce08d99fa8befc8c769226b01bcb5e80d9bf49228bbbe1adc8bc4693d5025a272d5
