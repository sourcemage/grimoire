   SOURCE_URL[0]=$SOURCEFORGE_URL/vamps/$SOURCE
     SOURCE_HASH=sha512:a068e5f5698e200e29a00700002ae34f3d19667d020f838d9d1f1ce8b0116b7b21e006c149682cdb3a0617e163b3b883f5e939930a33c5391d1eed4c82262aaf
