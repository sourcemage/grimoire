(

  . $SECTION_DIRECTORY/java-functions
  
  prepare_install 					&&
  
  extract_oth_src   ${SPELL} ${SPELL}-${VERSION}  $SOURCE_DIRECTORY	&&
  register_oth_jar  ${SPELL}-${VERSION}/lib/mx4j-jmx.jar  	mx4j-jmx.jar	&&
  register_oth_jar  ${SPELL}-${VERSION}/lib/mx4j-tools.jar  	mx4j-tools.jar	
  
) > $C_FIFO 2>&1  
