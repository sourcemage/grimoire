default_pre_build  &&

#
# Java binaries have hardcoded /lib64/ld-linux-x86-64.so.2
#
if [[ "${SMGL_COMPAT_ARCHS[1]}" == "x86_64" || "${SMGL_COMPAT_ARCHS[1]}" == "em64t" ]]
then
  if [[ ! -d $INSTALL_ROOT/lib64 ]] && [[ ! -h $INSTALL_ROOT/lib64 ]]
  then
    ln -sf $INSTALL_ROOT/lib $INSTALL_ROOT/lib64
  fi
fi
