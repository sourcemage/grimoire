         VERSION=0.6.3
      PATCHLEVEL=1
          SOURCE=$SPELL-$VERSION.tar.xz
	BASE_URL=http://kernel.org/pub/software/devel/$SPELL/dist
# Watch: https://kernel.org/pub/software/devel/sparse/dist/
   SOURCE_URL[0]=$BASE_URL/$SOURCE
     SOURCE_HASH=sha512:33170065ea5967d7c640a7551acc78d7efee9f7aa6a9edf6afe2cdd8a04ea5cebe7b5dcdeebf791d384542b8bcdd4e818c75e7ae2e7aa4e1edbd56b68367b28f
