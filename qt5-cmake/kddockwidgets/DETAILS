           SPELL=kddockwidgets
         VERSION=2.1.0
          SOURCE=$SPELL-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
        WEB_SITE=https://github.com/KDAB/KDDockWidgets
SOURCE_URL=$WEB_SITE/releases/download/v$VERSION/$SOURCE
         SOURCE2=$SOURCE.asc
  SOURCE2_URL[0]=${SOURCE_URL[0]}.asc
      SOURCE_GPG=kdab.gpg:$SOURCE.asc:UPSTREAM_KEY
  SOURCE2_IGNORE=signature
         ENTERED=20200120
      LICENSE[0]=GPL
        KEYWORDS="qt{5,6}"
           SHORT=" Dock Widget Framework for Qt{5,6}"
