mk_source_dir $SOURCE_DIRECTORY &&

cd $SOURCE_DIRECTORY            &&
mkdir 4.4 3.1 4.3 4.2           &&

cd $SOURCE_DIRECTORY/4.4        &&
unpack $SOURCE ${MD5[0]}        &&

cd $SOURCE_DIRECTORY/3.1        &&
unpack $SOURCE2 ${MD5[1]}       &&

cd $SOURCE_DIRECTORY/4.3        &&
unpack $SOURCE3 ${MD5[2]}       &&

cd $SOURCE_DIRECTORY/4.2        &&
unpack $SOURCE4 ${MD5[3]}
