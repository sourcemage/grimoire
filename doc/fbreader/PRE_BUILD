default_pre_build       &&
cd ${SOURCE_DIRECTORY}  &&

echo "TARGET_ARCH = desktop" >> makefiles/target.mk
if [ "$UI" = "qt" ]; then
  echo "UI_TYPE = qt" >> makefiles/target.mk
fi &&
if [ "$UI" = "gtk2" ]; then
  echo "UI_TYPE = gtk" >> makefiles/target.mk
fi &&

sed -i 's/Library::Library \&Library/Library \&Library/' fbreader/src/library/Library.cpp
